{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["esnext", "dom"],
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "downlevelIteration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
